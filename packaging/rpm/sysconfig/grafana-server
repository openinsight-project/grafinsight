GRAFINSIGHT_USER=grafinsight

GRAFINSIGHT_GROUP=grafinsight

GRAFINSIGHT_HOME=/usr/share/grafinsight

LOG_DIR=/var/log/grafinsight

DATA_DIR=/var/lib/grafinsight

MAX_OPEN_FILES=10000

CONF_DIR=/etc/grafinsight

CONF_FILE=/etc/grafinsight/grafinsight.ini

RESTART_ON_UPGRADE=true

PLUGINS_DIR=/var/lib/grafinsight/plugins

PROVISIONING_CFG_DIR=/etc/grafinsight/provisioning

# Only used on systemd systems
PID_FILE_DIR=/var/run/grafinsight
